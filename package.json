{
  "name": "Magpie",
  "version": "1.0.0",
  "description": "Electron 学习",
  "main": "./main/index.js",
  "scripts": {
    "start": "react-cli serve & node bin/copy-npm & NODE_ENV=development nodemon --exec electron ./main/index.js",
    "build": "rm -rf dist && react-cli build && node bin && npx electron-builder build"
  },
  "dependencies": {
    "@ke/form": "^2.1.2",
    "@ke/table": "^2.1.1",
    "@nbfe/tools": "^0.2.14",
    "antd": "^4.19.5",
    "axios": "^0.26.1",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "execa": "^5.0.0",
    "fix-path": "3.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.1",
    "highlight.js": "^11.5.1",
    "html2canvas": "^1.4.1",
    "ini": "^3.0.0",
    "ip": "^1.1.5",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-markdown": "^8.0.2",
    "react-router-dom": "5.2.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@jiaoyi/s3": "^0.1.7",
    "@ke/react-cli": "^1.2.0",
    "electron": "^18.0.1",
    "electron-builder": "^23.0.3",
    "nodemon": "^2.0.15"
  },
  "build": {
    "productName": "Magpie",
    "appId": "org.erb.shuoshubao",
    "files": [
      "package.json",
      "prettier.config.js",
      "public",
      "main",
      "server",
      "dist",
      "node_modules"
    ],
    "mac": {
      "category": "utilities",
      "target": [
        "dmg"
      ],
      "icon": "./public/icon.png"
    },
    "extends": null
  },
  "nodemonConfig": {
    "ignore": [
      "bin/**",
      "dist/**",
      "public/**",
      "src/**"
    ]
  }
}
